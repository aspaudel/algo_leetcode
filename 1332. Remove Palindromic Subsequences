class Solution {
    public int removePalindromeSub(String s) {
        String sr = "";
        for(int i = s.length()-1 ; i >= 0 ; i--) {
            sr += s.charAt(i);
        }
        for(int i = 0 ; i < s.length() ; i++) {
            if(s.charAt(i) != sr.charAt(i)) return 2; 
        }
        return 1;
    }
}
