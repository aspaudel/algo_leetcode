class Solution {
    public int maxArea(int[] height) {
        int l = 0, r = height.length-1;
        int sum = 0;
        while(l < r) {
            sum = Math.max(sum, (r-l) * Math.min(height[l], height[r]));
            if(height[l] > height[r]) r--;
            else l++;
        }
        return sum;
    }
}
