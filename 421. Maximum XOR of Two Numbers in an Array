class Trie {
    Trie zero;
    Trie one;
    Trie() {
        zero = null;
        one = null;
    }
}
class Solution {
    Trie root = new Trie();
    public void trieInsertion(String binaryString) {
        Trie temp = root;
        for(int i = 0 ; i < binaryString.length() ; i++) {
            if(binaryString.charAt(i) == '0') {
                if(temp.zero != null) {
                    temp = temp.zero;
                } else {
                    Trie zero = new Trie();
                    temp.zero = zero;
                    temp = temp.zero;
                }
            } else {
                if(temp.one != null) {
                    temp = temp.one;
                } else {
                    Trie one = new Trie();
                    temp.one = one;
                    temp = temp.one;
                }
            }
        }
    }
    public int findMaximumXOR(int[] nums) {
        List<String> arr = new ArrayList<>();
        for(int i = 0 ; i < nums.length ; i++) {
            String binaryString = String.format("%32s", Integer.toBinaryString(nums[i])).replace(' ',                   '0');               
            arr.add(binaryString);
            trieInsertion(binaryString);
        }
        int max = 0;
        for(int i = 0 ; i < nums.length ; i++) {
            String binaryString = arr.get(i);
            int j = 0;
            Trie temp = root;
            String s = "";
            while(j < 32) {
                if(temp.zero != null && binaryString.charAt(j) == '1') {
                    s += "1";
                    temp = temp.zero;
                } else if(temp.one != null && binaryString.charAt(j) == '0') {
                    s += "1";
                    temp = temp.one;
                } else if(temp.one != null && binaryString.charAt(j) == '1') {
                    s += "0";
                    temp = temp.one;
                }
                else if(temp.zero != null && binaryString.charAt(j) == '0') {
                    s += "0";
                    temp = temp.zero;
                }
                j++;
            }
            max = Math.max(max, Integer.parseInt(s, 2));
        }
        return max;
    }
}
