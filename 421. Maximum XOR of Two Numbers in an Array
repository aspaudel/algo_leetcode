!!!WARNING!!! Code not Working. Yet to debug!
class TrieNode {
    TrieNode left;
    TrieNode right;
    String val;
    TrieNode(String val) {
        this.val = val;
    }
}
class Solution {
    void trie_insertion(TrieNode root, String b) {
        String no = "";
        for(int i = 0 ; i < b.length() ; i++) {
            if(root.left != null && root.left.val.equals(Character.toString(b.charAt(i)))) {
                root = root.left;
                no += root.val;
                continue;
            }
            else if(root.right != null && root.right.val.equals(Character.toString(b.charAt(i)))) {
                root = root.right;
                no += root.val;
                continue;
            } else {
                TrieNode tn = new TrieNode(Character.toString(b.charAt(i)));
                if(root.left == null) {
                    root.left = tn;
                    root = root.left;
                    no += root.val;
                } else if(root.right == null) {
                    root.right = tn;
                    root = root.right;
                    no += root.val;
                }
            }
        }
        System.out.println(Integer.parseInt(no,2));
    }
    int maximumXor(TrieNode root, String b) {
        String max_xor = "";
        for(int i = 0 ; i < b.length() ; i++) {
            String opp_bit = "";
            if(b.charAt(i) == '0') {
                opp_bit = "1";
            }    
            else if(b.charAt(i) == '1') {
                opp_bit = "0";
            }
            if(root.left != null && root.left.val.equals(opp_bit)) {
                max_xor += opp_bit;
                root = root.left;
            } else if(root.right != null && root.right.val.equals(opp_bit)) {
                    max_xor += opp_bit;
                    root = root.right;
            } else {
                max_xor += Character.toString(b.charAt(i));
                if(root.left != null && root.left.val.equals(Character.toString(b.charAt(i)))) {
                    root = root.left;
                }
                if(root.right != null && root.right.val.equals(Character.toString(b.charAt(i)))) {
                    root = root.right;
                }
            }
        }
        System.out.println(max_xor+" "+Integer.parseInt(max_xor, 2));
        return Integer.parseInt(max_xor, 2);
    }
    public int findMaximumXOR(int[] nums) {
        TrieNode root = new TrieNode("root");
        for(int i = 0 ; i < nums.length ; i++) {
            String b = String.format("%32s", Integer.toBinaryString(nums[i])).replace(' ', '0');
            trie_insertion(root, b);    
        }
        int max = 0;
        for(int i = 0 ; i < nums.length ; i++) {
            String b = String.format("%32s", Integer.toBinaryString(nums[i])).replace(' ','0');
            max = Math.max(max, maximumXor(root, b));
        }
        return max;
    }
}
